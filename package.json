  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oblique-bit/oblique.git"
  },
  "scripts": {
    "lib": "npm run build -- oblique --watch",
    "start": "ng serve",
    "build": "ng build",
    "schematics": "npm run dist --prefix projects/oblique/schematics",
    "predist": "npm run gulp pre-dist",
    "dist": "npm run build -- oblique && npm run schematics",
    "distiOS": "npm run gulp pre-dist && npm run build -- oblique && npm run distiOS --prefix projects/oblique/schematics && npm run minify && npm run gulp -- dist",
    "postdist": "npm run minify && npm run gulp -- dist",
    "minify": "uglifyjs --compress --mangle --output dist/oblique/ob-features.js -- projects/oblique/src/ob-features.js",
    "test": "ng test oblique --prod",
    "lint": "ng lint oblique && ng lint oblique-showcase",
    "lint:fix": "ng lint oblique --fix && ng lint oblique-showcase --fix",
    "prettier": "./node_modules/.bin/prettier --write projects/**/{*.ts,*.html} src/**/{*.ts,*.html}",
    "format": "npm run lint:fix && npm run prettier",
    "themes": "npm run gulp -- themes",
    "icons": "ts-node scripts/icons.ts",
    "copy-error-messages": "node scripts/copy-error-messages.js",
    "e2e": "ng e2e",
    "release": "node scripts/release.js",
    "alpha": "npm run release -- alpha",
    "beta": "npm run release -- beta",
    "rc": "npm run release -- RC",
    "commit": "npm run gulp -- publish",
    "postinstall": "node scripts/sanitize-package-lock.js",
    "gulp": "./node_modules/.bin/gulp"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/hook.commit-rules",
      "pre-push": "node scripts/hook.push-clean"
    }
  },
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/localize": "^12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/material-moment-adapter": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@popperjs/core": "^2.9.2",
    "ajv": "^8.1.0",
    "ajv-formats": "^2.1.0",
    "jest-html-reporters": "^2.1.6",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.0",
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular-devkit/build-ng-packagr": "^0.1002.3",
    "@angular-eslint/builder": "^12.2.0",
    "@angular-eslint/eslint-plugin": "^12.2.0",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@schematics/angular": "^12.1.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "ansi-colors": "^4.1.1",
    "bootstrap": "^4.6.0",
    "conventional-changelog": "^3.1.24",
    "cpx": "^1.5.0",
    "del": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.0",
    "gulp-file": "^0.4.0",
    "gulp-git": "^2.10.0",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "moment": "^2.29.1",
    "ng-packagr": "^12.1.0",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.35.1",
    "ts-morph": "^9.1.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.2.4",
    "uglify-js": "^3.13.10"
  }
}
