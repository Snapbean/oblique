  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oblique-bit/oblique.git"
  },
  "scripts": {
    "lib": "npm run build -- oblique --watch",
    "start": "ng serve",
    "build": "ng build",
    "schematics": "npm run dist --prefix projects/oblique/schematics",
    "predist": "npm run gulp pre-dist && npm run build oblique-styles -- --configuration=production",
    "dist": "npm run build -- oblique && npm run schematics",
    "distiOS": "npm run gulp pre-dist && npm run build -- oblique && npm run distiOS --prefix projects/oblique/schematics && npm run minify && npm run gulp -- dist && ts-node scripts/post-dist.ts",
    "postdist": "npm run minify && npm run gulp -- dist && ts-node scripts/post-dist.ts",
    "minify": "uglifyjs --compress --mangle --output dist/oblique/ob-features.js -- projects/oblique/src/ob-features.js",
    "test": "ng test oblique --configuration production",
    "lint": "ng lint oblique && ng lint oblique-showcase",
    "format": "ng lint oblique --fix && ng lint oblique-showcase --fix",
    "icons": "ts-node scripts/icons.ts",
    "copy-error-messages": "ts-node scripts/copy-error-messages.ts",
    "e2e": "ng e2e",
    "release": "ts-node scripts/release.ts",
    "alpha": "npm run release -- alpha",
    "beta": "npm run release -- beta",
    "rc": "npm run release -- RC",
    "commit": "npm run gulp -- publish",
    "postinstall": "ts-node scripts/sanitize-package-lock.ts",
    "gulp": "./node_modules/.bin/gulp"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/hook.commit-rules",
      "pre-push": "node scripts/hook.push-clean"
    }
  },
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/localize": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/material-moment-adapter": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@popperjs/core": "^2.11.2",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.2",
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular-devkit/build-ng-packagr": "^0.1002.4",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@schematics/angular": "^13.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.5",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "ansi-colors": "^4.1.1",
    "bootstrap": "^4.6.1",
    "conventional-changelog": "^3.1.25",
    "cpx": "^1.5.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-git": "^2.10.0",
    "gulp-header": "^2.0.9",
    "gulp-replace": "^1.1.3",
    "husky": "^4.3.8",
    "jest": "27.4.3",
    "jest-html-reporters": "^2.1.7",
    "jest-sonar-reporter": "^2.0.0",
    "moment": "^2.29.1",
    "ng-packagr": "^13.1.3",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.4",
