{
  "$schema": "../../../schema.json",
  "api": [
    {
      "name": "ObButtonDirective",
      "usage": "This directive is to be used in conjunction with a <code>matButton</code> directive to give it an oblique style.",
      "selector": "[obButton]",
      "exported": "obButton",
      "type": "directive",
      "since": "6.0.0",
      "inputs": [
        {
          "name": "obButton",
          "text": "Indicates the button's type.",
          "type": "'primary' | 'secondary' | 'tertiary'",
          "default": "primary",
          "since": "6.0.0"
        }
      ]
    }
  ]
}
